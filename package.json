{
	"name": "bs-hero-section",
	"version": "1.0.0",
	"private": true,
	"description": "",
	"author": "@nclaudiuf",
	"license": "ISC",
	"homepage": "https://claudiu.design",
	"scripts": {
		"build:css": "npm run css",
		"css-compile": "node-sass --include-path node_modules --output-style compressed --source-map true --source-map-contents true --precision 6 scss -o dist/",
		"css-lint": "stylelint scss/",
		"css-prefix": "postcss --replace assets/css/section.css --use autoprefixer --map",
		"css-purge": "purgecss --keyframes --css assets/css/section.css --content index.html assets/js/main.js \"node_modules/bootstrap/js/dist/{modal,collapse,dropdown}.js\" --output dist/",
		"css": "npm-run-all css-compile css-prefix",
		"start": "parcel index.html --open",
		"build": "parcel build index.html",
		"watch": "nodemon -e html,scss -x \"npm run css\"",
		"test": "npm run css-lint && npm run css"
	},
	"dependencies": {
		"@parcel/transformer-image": "2.0.0-rc.0",
		"@popperjs/core": "2.10.1",
		"autoprefixer": "10.3.4",
		"bootstrap": "5.1.0",
		"bootstrap-icons": "1.5.0",
		"jquery": "3.6.0",
		"lozad": "1.16.0",
		"postcss": "8.3.6",
		"stylelint": "13.13.1",
		"stylelint-config-twbs-bootstrap": "2.2.3"
	},
	"devDependencies": {
		"parcel": "latest",
		"autoprefixer": "^10.2.5",
		"node-sass": "^5.0.0",
		"nodemon": "^2.0.7",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.2.10",
		"postcss-cli": "^8.3.1",
		"purgecss": "^4.0.2",
		"serve": "^11.3.2",
		"stylelint": "^13.12.0",
		"stylelint-config-twbs-bootstrap": "^2.1.0"
	},
	"keywords": []
}
